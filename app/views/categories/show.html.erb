<h1> Category: <%= @category.title %> </h1>

<h2> Projects: </h2>
<ul>
<% @category.projects.each do |project| %>
  <li><%= link_to project.title, project_path(project) %></li>
<% end %>
</ul>
<% if current_user.present? %>
</br></br>

<h2><%= link_to "Create a Project", new_project_path %></h2>
<% end %>
