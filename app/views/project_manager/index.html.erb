<h2> Project Manager </h2>
<% @categories.each do |category| %>
<h2> <%= link_to category.title, category_path(category) %> </h2>
  <ul>
  <% category.projects.each do |project| %>
    <li><%= link_to project.title, category_project_path(category, project) %></li>
    <ol><%=project.description%></ol>
  <% end %>
  </ul>
<% end %>
